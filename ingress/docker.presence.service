[Unit]
Description=Ingress Start for Presence Container
After=docker.service
Requires=docker.service

[Service]
TimeoutStartSec=0
Restart=always
EnvironmentFile=/etc/aurora/presence.conf
ExecStartPre=-/usr/bin/docker login -u $USER -p $PASSWORD
ExecStart=/usr/bin/docker run -p 6429:6429 --name=presence --entrypoint ./presence generalfusion/presence:latest -p $PACH_IP -c $DBCOLLECTION -d $DATABASE_NAME -u $MONGO_URL

[Install]
WantedBy=multi-user.target
